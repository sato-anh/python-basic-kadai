        "class Human:\n",
        "  def __init__(self):\n",
        "    self.name = \"\"\n",
        "    self.age = \"\"\n",
        "\n",
        "  def set_info(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def printinfo(self):\n",
        "    print(f\"名前：{self.name}, 年齢：{self.age}\")\n",
        "\n",
        "person = Human()\n",
        "\n",
        "person.set_info(\"Bob\", 20)\n",
        "person.printinfo()"
      ]
